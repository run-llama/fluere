{
  "name": "@llama-flow/core",
  "version": "0.3.4",
  "description": "event-based workflow engine",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "browser": "dist/esm/index.browser.js",
  "exports": {
    ".": {
      "workerd": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "edge-light": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.serverless.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./async-context": {
      "browser": {
        "types": "./dist/types/async-context/index.d.ts",
        "default": "./dist/esm/async-context/index.browser.js"
      },
      "require": {
        "types": "./dist/types/async-context/index.d.cts",
        "default": "./dist/cjs/async-context/index.cjs"
      },
      "default": {
        "types": "./dist/types/async-context/index.d.ts",
        "default": "./dist/esm/async-context/index.js"
      }
    },
    "./interrupter/hono": {
      "types": "./dist/types/interrupter/hono.d.ts",
      "default": "./dist/esm/interrupter/hono.js"
    },
    "./interrupter/mcp": {
      "types": "./dist/types/interrupter/mcp.d.ts",
      "default": "./dist/esm/interrupter/mcp.js"
    },
    "./interrupter/next": {
      "types": "./dist/types/interrupter/next.d.ts",
      "default": "./dist/esm/interrupter/next.js"
    },
    "./interrupter/promise": {
      "types": "./dist/types/interrupter/promise.d.ts",
      "default": "./dist/esm/interrupter/promise.js"
    },
    "./middleware/store": {
      "types": "./dist/types/middleware/store.d.ts",
      "default": "./dist/esm/middleware/store.js"
    },
    "./middleware/trace-events": {
      "types": "./dist/types/middleware/trace-events.d.ts",
      "default": "./dist/esm/middleware/trace-events.js"
    },
    "./middleware/validation": {
      "types": "./dist/types/middleware/validation.d.ts",
      "default": "./dist/esm/middleware/validation.js"
    },
    "./util/p-retry": {
      "types": "./dist/types/util/p-retry.d.ts",
      "default": "./dist/esm/util/p-retry.js"
    },
    "./util/zod": {
      "types": "./dist/types/util/zod.d.ts",
      "default": "./dist/esm/util/zod.js"
    },
    "./stream/consumer": {
      "types": "./dist/types/stream/consumer.d.ts",
      "default": "./dist/esm/stream/consumer.js"
    },
    "./stream/filter": {
      "types": "./dist/types/stream/filter.d.ts",
      "default": "./dist/esm/stream/filter.js"
    },
    "./stream/find": {
      "types": "./dist/types/stream/find.d.ts",
      "default": "./dist/esm/stream/find.js"
    },
    "./stream/until": {
      "types": "./dist/types/stream/until.d.ts",
      "default": "./dist/esm/stream/until.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "prepublishOnly": "cp ../../README.md ./README.md"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@rslib/core": "^0.6.5",
    "@microsoft/api-extractor": "^7.52.4",
    "@types/node": "^22.13.10",
    "hono": "^4.7.4",
    "next": "^15.2.2",
    "p-retry": "^6.2.1",
    "rimraf": "^6.0.1",
    "stream-chain": "^3.4.0",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "hono": "^4.7.4",
    "next": "^15.2.2",
    "p-retry": "^6.2.1",
    "zod": "^3.24.2"
  },
  "license": "MIT",
  "peerDependenciesMeta": {
    "@modelcontextprotocol/sdk": {
      "optional": true
    },
    "hono": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "p-retry": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/run-llama/llama-flow.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
